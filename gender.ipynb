      "cell_type": "code",
      "source": [
        "import torch\n",
        "\n",
        "# Carregar o modelo YOLOv5 customizado\n",
        "model = torch.hub.load('ultralytics/yolov5', 'custom', path=model_path, trust_repo=True)\n",
        "\n",
        "# Ajustar a confiança mínima para a detecção\n",
        "model.conf = 0.5\n"
      ],
